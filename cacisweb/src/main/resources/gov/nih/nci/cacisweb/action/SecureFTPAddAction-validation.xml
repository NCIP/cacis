<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<validator type="requiredstring">
		<param name="fieldname">secureFTPBean.certificateAlias</param>
		<message key="secureFTPBean.certificateAliasRequired"></message>
	</validator>
	<validator type="regex">
		<param name="fieldname">secureFTPBean.certificateAlias</param>
		<param name="expression"><![CDATA[^(sftp|ftps),((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])|(([a-zA-Z]|[a-zA-Z][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z]|[A-Za-z][A-Za-z0-9\\-]*[A-Za-z0-9])),[0-9]+,.*?,.*?,.*?]]></param>
		<message key="secureFTPBean.certificateAliasNotValid"></message>
	</validator>
	<!--<validator type="requiredstring">
		<param name="fieldname">secureFTPBean.certificateFileName</param>
		<message key="secureFTPBean.certificateRequired"></message>
	</validator> -->
</validators>