        <property name="script">
//load data into CDW

var cdwContext = new Packages.org.springframework.context.support.ClassPathXmlApplicationContext(&quot;classpath*:applicationContext-cacis-cdw.xml&quot;);
var loader = cdwContext.getBean(&quot;loader&quot;);

//call CDW loader after all the normal processing has occured
loader.load(messageObject.getEncodedData(), loader.generateContext(), $(&apos;studyId&apos;), $(&apos;siteId&apos;), $(&apos;patientId&apos;));

logger.info(&apos;Loading data into CDW&apos;);</property>
