                       context="*[cda:templateId/@root=&#34;2.16.840.1.113883.10.20.2.7&#34;]"/>

		    <!--ASSERT -->
<xsl:choose>
         <xsl:when test="cda:code/@code='18776-5'"/>
         <xsl:otherwise>
