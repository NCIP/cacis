
<profilesXml>

    <profiles>
        <profile>
            <id>local</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>

            <properties>
                <tolven.jboss.protocol>http</tolven.jboss.protocol>
                <tolven.jboss.hostname>localhost</tolven.jboss.hostname>
                <tolven.jboss.port>8080</tolven.jboss.port>
                <tolven.jboss.url>${tolven.jboss.protocol}://${tolven.jboss.hostname}:${tolven.jboss.port}
                </tolven.jboss.url>
                <tolven.jboss.configuration>tolven</tolven.jboss.configuration>

                <!--NOTE: customize this property for your tolven JBOSS location-->
                <tolven.jboss.location>${JBOSS_HOME}</tolven.jboss.location>
                <tolven.jboss.id>jboss42x</tolven.jboss.id>
                <tolven.jboss.server>${tolven.jboss.location}/server/${tolven.jboss.configuration}</tolven.jboss.server>

                <mirthConnect.cli>/Applications/Mirth Connect/mccommand</mirthConnect.cli>
                <mirthConnect.server.dir>/Applications/Mirth Connect</mirthConnect.server.dir>

		<outputRootdir>/Users/bpickeral/Documents</outputRootdir>
        	<cdfOutputDir>${outputRootdir}/cacis/mc/output/cdf/</cdfOutputDir>
        	<xccdOutputDir>${outputRootdir}/cacis/mc/output/xccd/</xccdOutputDir>
        	<trimInputDir>${outputRootdir}/cacis/mc/input/</trimInputDir>

		<!--  Temporary directory for System test output file -->
		<cacis.mc.temp.dir>/tmp</cacis.mc.temp.dir>

            </properties>
        </profile>


    </profiles>
</profilesXml>