<?xml version="1.0" encoding="UTF-8"?>
<trim xmlns="urn:tolven-org:trim:4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="urn:tolven-org:trim:4.0 http://tolven.org/xsd/trim4">
	<name>reg/evn/cacisOrganization</name>
	<menu>global:organizationMenu</menu>
	<application name="ephr">
		<instance>ephr:cacis:organization</instance>
		<wip>ephr:activity:all</wip>
	</application>
	<application name="echr">
		<instance>echr:cacis:organization</instance>
		<wip>echr:activity:all</wip>
	</application>
	<act classCode="REG" moodCode="EVN">
		<statusCode>active</statusCode>

		<title>
			<ST>Organization</ST>
		</title>

		<effectiveTime>
			<label>Date Measured</label>
			<new datatype="TS" function="now" />
			<TS>
				<value>20101221111354-0500</value>
			</TS>
		</effectiveTime>

		<!--Organization -->
		<participation typeCode="SBJ" name="subject">
			<role classCode="ACCESS">
				<bind application="echr" phase="create">
					<placeholder bindAction="merge">
						<path>echr:cacis:organization</path>
					</placeholder>
				</bind>
				<bind application="ephr" phase="create">
					<placeholder bindAction="merge">
						<path>echr:cacis:organization</path>
					</placeholder>
				</bind>
				<id>
					<II>
						<root>1.2.3</root>
						<extension>1</extension>
					</II>
				</id>
				<player classCode="ORG" determinerCode="INSTANCE">
					<label>PRPMMT000008USOrganization</label>
					<bind application="echr" phase="create">
						<placeholder bindAction="merge">
							<path>echr:cacis:organization:name</path>
						</placeholder>
					</bind>
					<bind application="ephr" phase="create">
						<placeholder bindAction="merge">
							<path>echr:cacis:organization:name</path>
						</placeholder>
					</bind>					
                    <id>
                        <II>
                            <root>1.2.4</root>
                            <extension>2</extension>
                        </II>
                    </id>
					<code>
						<CE>
							<displayName>codeNm</displayName>
							<code>code</code>
							<codeSystem>codesystem</codeSystem>
							<codeSystemName>codesystemname</codeSystemName>
							<codeSystemVersion>codesystemversion</codeSystemVersion>
						</CE>
					</code>
					<statusCode>active</statusCode>
					<name>
						<label>Organization Name</label>
						<EN>
							<use>L</use>
							<part>
								<label>Organization Name</label>
								<ST>org-name</ST>
							</part>
						</EN>
					</name>
					<telecom>
						<label>Telecom</label>
						<TEL>
							<label></label>
							<use>L</use>
							<value>9876543210</value>
						</TEL>
					</telecom>
					<organization>
						<addr>
							<label>Address</label>
							<AD>
								<label>Org Location</label>
								<use>L</use>
								<part>
									<label>Address Line</label>
									<type>ADL</type>
									<ST>address line 1</ST>
								</part>
							</AD>
						</addr>
					</organization>
				</player>
			</role>
			<!--
				The following relationships will not be provided and are optional.
				So, leaving them unspecified. If they are made as required, the we
				need to include them as trim blending 1) asPartOfWhole - Should be
				this trim itself blended 2) serviceDeliveryLocation - Should be a
				new SDL trim defined and blended here 3) partOfWhole - Should be
				this trim itself blended
			-->
		</participation>
	</act>
</trim>
